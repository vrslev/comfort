{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:8001",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Bench Web",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/../frappe/frappe/utils/bench_helper.py",
      "args": [
        "frappe",
        "serve",
        "--port",
        "8000",
        "--noreload",
        "--nothreading"
      ],
      "python": "${workspaceFolder}/../../env/bin/python",
      "cwd": "${workspaceFolder}/../../sites",
      "env": {
        "DEV_SERVER": "1"
      }
    }
  ]
}
